<ul class="List">
		{{#each items as |post|}}
		<li class="Media Media-container">
			<figure class="Media-figure">
				{{#link-to 'news.post' post class="Media"}}
				<img src="{{post.better_featured_image.media_details.sizes.l.source_url}}" alt="{{post.better_featured_image.alt}}"> {{/link-to}}
			</figure>
		
		</li>
		{{/each}}

		
</ul>

{{yield}}




			