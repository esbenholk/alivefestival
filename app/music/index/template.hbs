<div class="Manchet">
	<h1 class="Title">{{model.page.title}}</h1>
	{{{model.page.content}}}
</div>

<div class="Artists">
	{{#masonry-grid items=model.artists as |item|}}
		{{#link-to 'music.artist' item}}
			{{teaser-item item=item}}
		{{/link-to}}
	{{/masonry-grid}}
